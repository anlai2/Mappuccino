{
  "expo": {
    "name": "Mappuccino",
    "description": "Find coffee shops near you with a single button.",
    "slug": "mappuccino",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/mappuccinoIconApple.png",
    "splash": {
      "image": "./assets/mappuccinoLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.andy-lai.mappuccino",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to initially focus the search area around you and automatically use your source address for directions. Do you allow us to use location service on this device?"
      }
    },
    "android": {
      "package": "com.andylai.mappuccino",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBTx9JiRSCODm2i65IPuWi_uiZYVhZghcM"
        }
      }
    }
  }
}
